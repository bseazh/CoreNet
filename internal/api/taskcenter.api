type CreateOCRReq {
	FileId string `json:"fileId"`
}

type CreateOCRResp {
	JobId string `json:"jobId"`
}

type JobResp {
	JobId     string `json:"jobId"`
	Status    string `json:"status"`
	ResultURI string `json:"resultURI,optional"`
}

@server (
	group: taskcenter
)
service taskcenter {
	@handler CreateOCR
	post /tasks/ocr (CreateOCRReq) returns (CreateOCRResp)

	@handler GetJob
	get /tasks/:jobId returns (JobResp)
}

